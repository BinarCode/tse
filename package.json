{
    "name": "starter",
    "version": "1.0.0",
    "main": "cluster.js",
    "scripts": {
        "build": "bin/build.sh",
        "build-ts": "tsc",
        "clean": "rm -rf dist node_modules",
        "sequelize": "cross-env NODE_PATH=. sequelize",
        "start": "node cluster.js",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "tslint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
        "format": "prettier --write \"./**/*{.ts,.js}\"",
        "format:check": "prettier --list-different \"src/**/*{.ts,.js}\"",
        "serve-debug": "nodemon --inspect dist/server.js",
        "dev": "NODE_ENV=development ts-node-dev --inspect --respawn --transpileOnly --ignore-watch node_modules ./index.ts",
        "heroku-postbuild": "bin/build.sh",
        "db:promote": "bin/heroku-db-promote.sh",
        "db:prepare": "bin/db_prepare.sh",
        "db:prepare:seed": "bin/db_prepare_seed.sh",
        "db:seed": "sequelize db:seed:all",
        "db:seed:rollback": "sequelize db:seed:undo",
        "db:seed:rollback:all": "sequelize db:seed:undo:all",
        "db:drop": "sequelize db:drop",
        "db:create": "sequelize db:create",
        "db:migrate": "sequelize db:migrate",
        "db:migrate:rollback": "sequelize db:migrate:undo",
        "db:migrate:rollback:all": "sequelize db:migrate:undo:all",
        "db:refresh": "sequelize db:migrate:undo:all && npm run db:migrate && npm run db:seed",
        "db:seed:refresh": "npm run db:seed:rollback:all && npm run db:seed",
        "db:make-migration": "bin/make-migration.sh"
    },
    "dependencies": {
        "@hapi/joi": "^15.1.0",
        "@types/bluebird": "^3.5.27",
        "@types/body-parser": "^1.17.0",
        "@types/compression": "0.0.36",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.0",
        "@types/express-winston": "^3.0.3",
        "@types/joi": "14.3.3",
        "@types/node": "^12.0.12",
        "@types/nodemailer": "^6.2.0",
        "@types/sequelize": "^4.28.3",
        "@types/sharp": "0.22.2",
        "@types/stripe": "^6.31.4",
        "bcrypt": "^3.0.6",
        "bluebird": "^3.5.4",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "config": "^3.1.0",
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "errorhandler": "^1.5.1",
        "express": "4.16.4",
        "express-fileupload": "^1.1.5",
        "express-status-monitor": "^1.2.6",
        "faker": "^4.1.0",
        "helmet": "^3.18.0",
        "http-status": "^1.3.2",
        "i18next": "^17.0.9",
        "i18next-express-middleware": "^1.8.0",
        "i18next-node-fs-backend": "^2.1.3",
        "is-class": "0.0.7",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.10",
        "mem": "^5.1.1",
        "method-override": "^3.0.0",
        "mjml": "^4.4.0-beta.2",
        "module-alias": "^2.2.0",
        "moment": "^2.24.0",
        "nodemailer": "^6.2.1",
        "nodemon": "1.19.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "pg": "^7.11.0",
        "pg-hstore": "^2.3.3",
        "pm2": "^3.5.1",
        "pug": "^2.0.4",
        "readline": "^1.3.0",
        "redis": "^2.8.0",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "require-all": "^3.0.0",
        "sequelize": "^5.9.2",
        "sequelize-cli": "^5.5.0",
        "sequelize-slugify": "^0.7.0",
        "sharp": "^0.22.1",
        "string.prototype.padstart": "^3.0.0",
        "stripe": "^7.4.0",
        "ts-node-dev": "^1.0.0-pre.40",
        "tslint": "^5.18.0",
        "typescript": "^3.5.2",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/faker": "^4.1.5",
        "@types/hapi__joi": "^15.0.2",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/passport": "^1.0.0",
        "@types/passport-jwt": "^3.0.1",
        "chalk": "^2.4.2",
        "cross-env": "^5.2.0",
        "husky": "^2.7.0",
        "path": "^0.12.7",
        "prettier": "1.18.2",
        "pretty-quick": "^1.11.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.18.0"
    },
    "engines": {
        "node": "10.x"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
